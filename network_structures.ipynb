    "from os import listdir                                                                                  #Import benötigter Module\n",
    "from os.path import isfile, join\n",
    "import pandas as pd\n",
    "\n",
    "\n",
    "videos = [f for f in listdir(\"video_csvs/\") if isfile(join(\"video_csvs/\", f))]                          #Alle Dateien aus Ordner \"video_csvs\" auslesen und Liste \"videos\" hinzufügen\n",
    "if \".DS_Store\" in videos:                                                        \n",
    "    videos.remove(\".DS_Store\")                                                                          #störende Datei entfernen (Erklärung folgt in Abschnitt \"Code\")\n",
    "\n",
    "for video in videos:\n",
    "    video_infos = pd.read_csv(\"video_csvs/\"+video)                                                      \n",
    "    video_infos.rename(columns={video_infos.columns[1]: \"Info\"}, inplace=True)                          #Titel der 2. Spalte jeder Datei variiert, weshalb er für das nachstehende Auslesen vereinheitlicht (\"Info\") werden muss\n",
    "\n",
    "    channel = video_infos.loc[video_infos[\"id\"].str.contains(\"channelTitle\"), \"Info\"].values[0]         #Spalten mit den gewünschten Informationen (channelTitle, commentCount) suchen und entsprechende Variable speichern\n",
    "    comment_count = video_infos.loc[video_infos[\"id\"].str.contains(\"commentCount\"), \"Info\"].values[0]\n",
    "    view_count = video_infos.loc[video_infos[\"id\"].str.contains(\"viewCount\"), \"Info\"].values[0]\n",
    "\n",
    "    print(channel+ \" | Aufrufe: \" + view_count + \" | Kommentare: \" + comment_count)                     #Alle Variablen mit Kontext ausgeben\n",
    "    \n"
